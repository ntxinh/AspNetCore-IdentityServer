  "version": "0.2.0",
  "configurations": [
    {
      "name": "IdentityServer-Dev",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/IdentityServer/bin/Debug/netcoreapp3.1/IdentityServer.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/IdentityServer",
      "stopAtEntry": false,
      "console": "internalConsole",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:5001/"
      },
    },
    {
      "name": "Api-Dev",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Api/bin/Debug/netcoreapp3.1/Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Api",
      "stopAtEntry": false,
      "console": "internalConsole",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:6001/"
      },
    }
  ],
  "compounds": [
    {
      "name": "Run-All-Dev",
      "configurations": [
        "IdentityServer-Dev",
        "Api-Dev"
      ]
    }
  ]
}
